-- Finds items by a keyword query and/or category and allows searching within item descriptions.
return select searchResult.item, errorMessage
from ebay.finding.findItemsAdvanced where affiliate.customId = '{affiliatecustomId}' and affiliate.geoTargeting = '{affiliategeoTargeting}' and affiliate.networkId = '{affiliatenetworkId}' and affiliate.trackingId = '{affiliatetrackingId}' and buyerPostalCode = '{buyerPostalCode}' and paginationInput.entriesPerPage = '{paginationInputentriesPerPage}' and paginationInput.pageNumber = '{paginationInputpageNumber}' and sortOrder = '{sortOrder}' and aspectFilter.aspectName = '{aspectFilteraspectName}' and aspectFilter.aspectValue = '{aspectFilteraspectValue}' and categoryId = '{categoryId}' and descriptionSearch = '{descriptionSearch}' and domainFilter.domainName = '{domainFilterdomainName}' and keywords = '{keywords}' and outputSelector = '{outputSelector}'
via route '/ebay/finding/advanced?affiliatecustomid={affiliatecustomId}&affiliategeotargeting={affiliategeoTargeting}&affiliatenetworkid={affiliatenetworkId}&affiliatetrackingid={affiliatetrackingId}&buyerpostalcode={buyerPostalCode}&paginationinputentriesperpage={paginationInputentriesPerPage}&paginationinputpagenumber={paginationInputpageNumber}&sortorder={sortOrder}&aspectfilteraspectname={aspectFilteraspectName}&aspectfilteraspectvalue={aspectFilteraspectValue}&categoryid={categoryId}&descriptionsearch={descriptionSearch}&domainfilterdomainname={domainFilterdomainName}&keywords={keywords}&outputselector={outputSelector}'
with optional params
using method get;
