-- Finds items by a keyword query and/or category and allows searching within item descriptions.
return select searchResult.item, errorMessage
from ebay.finding.findItemsAdvanced where affiliate.customId = '{affiliate.customId}' and affiliate.geoTargeting = '{affiliate.geoTargeting}' and affiliate.networkId = '{affiliate.networkId}' and affiliate.trackingId = '{affiliate.trackingId}' and buyerPostalCode = '{buyerPostalCode}' and paginationInput.entriesPerPage = '{paginationInput.entriesPerPage}' and paginationInput.pageNumber = '{paginationInput.pageNumber}' and sortOrder = '{sortOrder}' and aspectFilter.aspectName = '{aspectFilter.aspectName}' and aspectFilter.aspectValue = '{aspectFilter.aspectValue}' and categoryId = '{categoryId}' and descriptionSearch = '{descriptionSearch}' and domainFilter.domainName = '{domainFilter.domainName}' and affiliate.customId = '{affiliate.customId}' and affiliate.geoTargeting = '{affiliate.geoTargeting}' and affiliate.networkId = '{affiliate.networkId}' and affiliate.trackingId = '{affiliate.trackingId}' and keywords = '{keywords}' and outputSelector = '{outputSelector}'
via route '/ebay/trading/finditemsadvanced?customId={affiliate.customId}&geoTargeting={affiliate.geoTargeting}&networkId={affiliate.networkId}&trackingId={affiliate.trackingId}&buyerPostalCode={buyerPostalCode}&entriesPerPage={paginationInput.entriesPerPage}&pageNumber={paginationInput.pageNumber}&sortOrder={sortOrder}&aspectName={aspectFilter.aspectName}&aspectValue={aspectFilter.aspectValue}&categoryId={categoryId}&descriptionSearch={descriptionSearch}&domainName={domainFilter.domainName}&customId={affiliate.customId}&geoTargeting={affiliate.geoTargeting}&networkId={affiliate.networkId}&trackingId={affiliate.trackingId}&keywords={keywords}&outputSelector={outputSelector}'
with optional param
using method get;
