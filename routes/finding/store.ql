-- Finds items in eBay stores. Can search a specific store or can search all stores with a keyword query.
return select searchResult.item, errorMessage
from ebay.finding.findItemsIneBayStores where affiliate.customId = '{affiliatecustomId}' and affiliate.geoTargeting = '{affiliategeoTargeting}' and affiliate.networkId = '{affiliatenetworkId}' and affiliate.trackingId = '{affiliatetrackingId}' and buyerPostalCode = '{buyerPostalCode}' and paginationInput.entriesPerPage = '{paginationInputentriesPerPage}' and paginationInput.pageNumber = '{paginationInputpageNumber}' and sortOrder = '{sortOrder}' and aspectFilter.aspectName = '{aspectFilteraspectName}' and aspectFilter.aspectValueName = '{aspectFilteraspectValueName}' and categoryId = '{categoryId}' and domainFilter.domainName = '{domainFilterdomainName}' and itemFilter.paramName = '{itemFilterparamName}' and itemFilter.paramValue = '{itemFilterparamValue}' and itemFilter.value = '{itemFiltervalue}' and keywords = '{keywords}' and outputSelector = '{outputSelector}' and storeName = '{storeName}'
via route '/ebay/finding/stores?affiliatecustomid={affiliatecustomId}&affiliategeotargeting={affiliategeoTargeting}&affiliatenetworkid={affiliatenetworkId}&affiliatetrackingid={affiliatetrackingId}&buyerpostalcode={buyerPostalCode}&paginationinputentriesperpage={paginationInputentriesPerPage}&paginationinputpagenumber={paginationInputpageNumber}&sortorder={sortOrder}&aspectfilteraspectname={aspectFilteraspectName}&aspectfilteraspectvaluename={aspectFilteraspectValueName}&categoryid={categoryId}&domainfilterdomainname={domainFilterdomainName}&itemfilterparamname={itemFilterparamName}&itemfilterparamvalue={itemFilterparamValue}&itemfiltervalue={itemFiltervalue}&keywords={keywords}&outputselector={outputSelector}&storename={storeName}'
with optional params
using method get;

